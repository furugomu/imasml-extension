{
  "name": "imasml-extension",
  "version": "0.1.0",
  "description": "二階堂千鶴",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "http://imas.gree-apps.net/",
        "http://imas.gree-apps.net/app/index.php/*"
      ],
      "js": [
        "content-script.js"
      ]
    }
  ],
  "permissions": [
    "tabs",
    "webRequest", "webRequestBlocking",
    "storage",
    "http://imas.gree-apps.net/*",
    "*://*.gree.net/*"
  ],
  "manifest_version": 2
}
